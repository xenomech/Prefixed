{
  "name": "prefixed",
  "displayName": "Prefixed",
  "description": "This Extention allows you to prefix ticket id from your branch name to your commit message.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:prefixed.prefixTicketID"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "prefixed.prefixTicketID",
      "title": "Prefix"
    }],
    "menus": {
			"scm/title": [
				{
					"command": "prefixed.prefixTicketID",
					"when": "scmProvider == git",
					"group": "navigation"
				}
			]
		}
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
